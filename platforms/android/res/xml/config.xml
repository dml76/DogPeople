    <gap:splash gap:platform="ios" height="2048" src="www/img/launch/ios/Default-Portrait@2x.png" width="1536" />
    <gap:splash gap:platform="ios" height="1536" src="www/img/launch/ios/Default-Landscape@2x.png" width="2048" />
    <content src="index.html" />
    <access origin="*" />
</widget>
