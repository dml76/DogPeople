<?xml version='1.0' encoding='utf-8'?>
<widget id="com.dmlapps.dogpeople" version="2.3" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <name>DogPeople</name>
    <description>Everything for Fido</description>
    <author email="info@dmlapps.com" href="http://dmlapps.com/dogpeople">Team DML</author>
    <preference name="permissions" value="none" />
    <gap:plugin name="org.apache.cordova.geolocation" />
    <gap:plugin name="org.apache.cordova.statusbar" version="0.1.4" />
    <preference name="StatusBarOverlaysWebView" value="true" />
    <plugins>
        <plugin name="Device" value="CDVDevice" />
        <plugin name="Logger" value="CDVLogger" />
        <plugin name="Compass" value="CDVLocation" />
        <plugin name="NetworkStatus" value="CDVConnection" />
        <plugin name="Debug Console" value="CDVDebugConsole" />
        <plugin name="Geolocation" value="CDVLocation" />
        <plugin name="SplashScreen" value="CDVSplashScreen" />
        <plugin name="Battery" value="CDVBattery" />
        <plugin name="Globalization" value="CDVGlobalization" />
    </plugins>
    <gap:platform name="ios" />
    <gap:platform name="android" />
    <icon src="icon.png" />
    <gap:splash src="splash.png" />
    <icon gap:density="ldpi" gap:platform="android" src="www/img/icons/android/ldpi.png" />
    <icon gap:density="mdpi" gap:platform="android" src="www/img/icons/android/mdpi.png" />
    <icon gap:density="hdpi" gap:platform="android" src="www/img/icons/android/hdpi.png" />
    <icon gap:density="xhdpi" gap:platform="android" src="www/img/icons/android/xdpi.png" />
    <icon gap:density="xxhdpi" gap:platform="android" src="www/img/icons/android/xxdpi.png" />
    <gap:splash gap:platform="android" gap:qualifier="ldpi" src="www/img/launch/android/splash-land-ldpi.png" />
    <gap:splash gap:platform="android" gap:qualifier="mdpi" src="www/img/launch/android/splash-land-mdpi.png" />
    <gap:splash gap:platform="android" gap:qualifier="hdpi" src="www/img/launch/android/splash-land-hdpi.png" />
    <gap:splash gap:platform="android" gap:qualifier="xhdpi" src="www/img/launch/android/splash-land-xhdpi.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-xxhdpi" src="www/img/launch/android/portrait-xxhdpi.png" />
    <gap:splash gap:platform="android" gap:qualifier="land-xxhdpi" src="www/img/launch/android/landscape-xxhdpi.png" />
    <icon gap:platform="ios" height="180" src="www/img/icons/ios/icon-60@3x.png" width="180" />
    <icon gap:platform="ios" height="60" src="www/img/icons/ios/icon-60.png" width="60" />
    <icon gap:platform="ios" height="120" src="www/img/icons/ios/icon-60@2x.png" width="120" />
    <icon gap:platform="ios" height="76" src="www/img/icons/ios/icon-76.png" width="76" />
    <icon gap:platform="ios" height="152" src="www/img/icons/ios/icon-76@2x.png" width="152" />
    <icon gap:platform="ios" height="29" src="www/img/icons/ios/icon-small.png" width="29" />
    <icon gap:platform="ios" height="58" src="www/img/icons/ios/icon-small@2x.png" width="58" />
    <icon gap:platform="ios" height="40" src="www/img/icons/ios/icon-40.png" width="40" />
    <icon gap:platform="ios" height="80" src="www/img/icons/ios/icon-40@2x.png" width="80" />
    <icon gap:platform="ios" height="57" src="www/img/icons/ios/icon.png" width="57" />
    <icon gap:platform="ios" height="114" src="www/img/icons/ios/icon@2x.png" width="114" />
    <icon gap:platform="ios" height="72" src="www/img/icons/ios/icon-72.png" width="72" />
    <icon gap:platform="ios" height="144" src="www/img/icons/ios/icon-72@2x.png" width="144" />
    <icon gap:platform="ios" height="50" src="www/img/icons/ios/icon-50.png" width="50" />
    <icon gap:platform="ios" height="100" src="www/img/icons/ios/icon-50@2x.png" width="100" />
    <gap:splash gap:platform="ios" height="480" src="www/img/launch/ios/Default.png" width="320" />
    <gap:splash gap:platform="ios" height="960" src="www/img/launch/ios/Default@2x.png" width="640" />
    <gap:splash gap:platform="ios" height="1136" src="www/img/launch/ios/Default-568h@2x.png" width="640" />
    <gap:splash gap:platform="ios" height="1334" src="www/img/launch/ios/Default-667h@2x.png" width="750" />
    <gap:splash gap:platform="ios" height="2208" src="www/img/launch/ios/Default-Portrait-736h@3x.png" width="1242" />
