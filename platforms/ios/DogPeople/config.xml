<?xml version='1.0' encoding='utf-8'?>
<widget id="com.dmlapps.dogpeople" version="3.4" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="Geolocation">
        <param name="ios-package" value="CDVLocation" />
    </feature>
    <name>DogPeople</name>
    <description>Everything for Fido</description>
    <author email="info@dmlapps.com" href="http://dmlapps.com/dogpeople">Team DML</author>
    <gap:plugin name="org.apache.cordova.geolocation" />
    <gap:plugin name="org.apache.cordova.statusbar" version="0.1.4" />
    <preference name="StatusBarOverlaysWebView" value="true" />
    <plugins>
        <plugin name="Device" value="CDVDevice" />
        <plugin name="Logger" value="CDVLogger" />
        <plugin name="Compass" value="CDVLocation" />
        <plugin name="NetworkStatus" value="CDVConnection" />
        <plugin name="Debug Console" value="CDVDebugConsole" />
        <plugin name="Geolocation" value="CDVLocation" />
        <plugin name="SplashScreen" value="CDVSplashScreen" />
        <plugin name="Battery" value="CDVBattery" />
        <plugin name="Globalization" value="CDVGlobalization" />
    </plugins>
    <gap:platform name="ios" />
    <gap:platform name="android" />
    <icon gap:role="default" src="icon_72.png" />
    <icon height="57" src="icon.png" width="57" />
    <icon height="48" src="icon_48.png" width="48" />
    <icon height="36" src="icon_36.png" width="36" />
    <gap:splash src="splash.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-ldpi" src="www/img/launch/android/screen-ldpi-portrait.png" />
    <gap:splash gap:platform="android" gap:qualifier="land-ldpi" src="www/img/launch/android/screen-ldpi-landscape.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-mdpi" src="www/img/launch/android/screen-mdpi-portrait.png" />
    <gap:splash gap:platform="android" gap:qualifier="land-mdpi" src="www/img/launch/android/screen-mdpi-landscape.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-hdpi" src="www/img/launch/android/screen-hdpi-portrait.png" />
    <gap:splash gap:platform="android" gap:qualifier="land-hdpi" src="www/img/launch/android/screen-hdpi-landscape.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-xhdpi" src="www/img/launch/android/screen-xhdpi-portrait.png" />
    <gap:splash gap:platform="android" gap:qualifier="land-xhdpi" src="www/img/launch/android/screen-xhdpi-landscape.png" />
