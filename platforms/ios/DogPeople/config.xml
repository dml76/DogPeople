<?xml version='1.0' encoding='utf-8'?>
<widget id="com.dmlapps.dogpeople" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="UniversalAnalytics">
        <param name="ios-package" value="UniversalAnalyticsPlugin" />
    </feature>
    <icon height="180" src="www/img/icons/ios/icon-60@3x.png" width="180" />
    <icon height="120" src="www/img/icons/ios/icon-60@2x.png" width="120" />
    <icon height="114" src="www/img/icons/ios/icon@2x.png" width="114" />
    <icon height="80" src="www/img/icons/ios/icon-40@2x.png" width="80" />
    <icon height="60" src="www/img/icons/ios/icon-60.png" width="60" />
    <icon height="58" src="www/img/icons/ios/icon-small@2x.png" width="58" />
    <icon height="57" src="www/img/icons/ios/icon.png" width="57" />
    <icon height="40" src="www/img/icons/ios/icon-40.png" width="40" />
    <icon height="29" src="www/img/icons/ios/icon-small.png" width="29" />
    <splash height="2208" src="www/img/launch/ios/Default-736h.png" width="1242" />
    <splash height="2048" src="www/img/launch/ios/Default-Portrait@2x~ipad.png" width="1536" />
    <splash height="1334" src="www/img/launch/ios/Default-667h.png" width="750" />
    <splash height="1136" src="www/img/launch/ios/Default-568h@2x~iphone.png" width="640" />
